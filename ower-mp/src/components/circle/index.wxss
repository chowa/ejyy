@import '../common/index.wxss';

.cw-circle {
    position: relative;
    display: inline-block;
    text-align: center;
}

.cw-circle__text {
    position: absolute;
    top: 50%;
    left: 0;
    width: 100%;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
    color: #323233;
}
